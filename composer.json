{
    "name": "yuges-code/laravel-mediable",
    "description": "ðŸŽ´ Package for easily uploading and attaching media files to Laravel eloquent models",
    "keywords": [
        "php",
        "yuges",
        "medias",
        "laravel",
        "package",
        "mediable"
    ],
    "homepage": "https://yuges-code.github.io/laravel-mediable",
    "support": {
        "source": "https://github.com/yuges-code/laravel-mediable",
        "issues": "https://github.com/yuges-code/laravel-mediable/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Georgy",
            "email": "goshasafonov@yandex.ru",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.4",
        "ext-exif": "*",
        "ext-imagick": "*",
        "yuges-code/image-php": "*",
        "yuges-code/laravel-package": "^1.1",
        "yuges-code/laravel-orderable": "^1.0"
    },
    "require-dev": {
        "orchestra/testbench": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Yuges\\Mediable\\": "src",
            "Yuges\\Mediable\\Database\\Seeders\\": "database/seeders/",
            "Yuges\\Mediable\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yuges\\Mediable\\Tests\\": "tests/src"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Yuges\\Mediable\\Providers\\MediableServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
